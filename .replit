modules = ["dotnet-7.0", "web", "nodejs-20", "dotnet-9.0", "postgresql-16"]
[agent]
expertMode = true
integrations = ["javascript_gemini:1.0.0"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "FantasyToolbox Server"

[[workflows.workflow]]
name = "FantasyToolbox Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "dotnet run --urls \"http://0.0.0.0:5000\""
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 38707
externalPort = 3001

[[ports]]
localPort = 43757
externalPort = 3000

[deployment]
deploymentTarget = "autoscale"
run = ["dotnet", "run", "--urls", "http://0.0.0.0:5000"]
